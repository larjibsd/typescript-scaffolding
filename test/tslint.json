{
  "defaultSeverity": "error",
  "extends": [
    "../tslint.json"
  ],
  "rules": {
    "no-console": false,
    "typedef": false,
    "variable-name": [
      true,
      "allow-leading-underscore",
      "allow-trailing-underscore",
      "allow-pascal-case",
      "ban-keywords",
      "check-format"
    ]
  }
}
